{
    "parserOptions": {
        "ecmaVersion": 2016
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "no-mixed-spaces-and-tabs": "error",
        "no-var": "error",
        "no-unused-vars": ["error", {
            "vars": "all",
            "args": "after-used"
        }]
    },
    "globals": {
        "angular": true,
        "io": true,
        "Proxy": true
    }
}